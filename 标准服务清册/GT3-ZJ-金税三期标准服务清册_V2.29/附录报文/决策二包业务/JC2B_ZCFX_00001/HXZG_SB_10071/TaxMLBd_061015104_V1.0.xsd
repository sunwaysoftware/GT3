			<xs:documentation>企业重组所得税特殊性税务处理报告表业务表单</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="taxDoc">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:element name="qyczGridlb" type="qyczbdxxVO">
						<xs:annotation>
							<xs:documentation>居民企业资产（股权）划转特殊性税务处理申报表_主表</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
