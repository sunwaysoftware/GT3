<xs:schema xmlns="http://www.chinatax.gov.cn/dataspec/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.chinatax.gov.cn/dataspec/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="TaxML.xsd"/>
	<xs:include schemaLocation="TaxMLPublicType.xsd"/>
	<xs:complexType name="gygqwczrghdjbVO">
		<xs:annotation>
			<xs:documentation>过户登记表（适用上市公司国有股权无偿转让）</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="nsrxxForm" type="NsrxxFormVO">
				<xs:annotation>
					<xs:documentation>纳税人基本信息</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ghdjbxxGrid">
				<xs:annotation>
					<xs:documentation>过户登记表信息</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:element name="ghdjbxxGridlb" type="ghdjbxxGridlbVO">
							<xs:annotation>
								<xs:documentation>过户登记表信息列表</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ghdjbxxGridlbVO">
		<xs:annotation>
			<xs:documentation>过户登记表信息（适用上市公司国有股权无偿转让）</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="sqrmc" type="sqrmc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>申请人名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="swdjh" type="swdjh" minOccurs="0">
				<xs:annotation>
					<xs:documentation>申请人税务登记号码</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sqrszd" type="sqrszd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>申请人所在地</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pzwh" type="pzwh" minOccurs="0">
				<xs:annotation>
					<xs:documentation>批准文号</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gpmc" type="gpmc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>划转股票名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hzgs" type="hzgs" minOccurs="0">
				<xs:annotation>
					<xs:documentation>划转股数</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hcfqymc" type="hcfqymc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>划出方企业名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hrfqymc" type="hrfqymc" minOccurs="0">
				<xs:annotation>
					<xs:documentation>划入方企业名称</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jbrqz" type="jbrqz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>经办人签字</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sbrq" type="sbrq" minOccurs="0">
				<xs:annotation>
					<xs:documentation>申办日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ghrq" type="ghrq" minOccurs="0">
				<xs:annotation>
					<xs:documentation>过户日期</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="djjsgsjbrqz" type="djjsgsjbrqz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>登记结算公司经办人签字</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bz" type="bz" minOccurs="0">
				<xs:annotation>
					<xs:documentation>备注</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
