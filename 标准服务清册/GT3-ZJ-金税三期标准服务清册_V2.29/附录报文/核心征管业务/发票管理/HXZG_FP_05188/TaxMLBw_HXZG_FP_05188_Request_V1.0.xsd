<xs:schema xmlns="http://www.chinatax.gov.cn/dataspec/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.chinatax.gov.cn/dataspec/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="TaxDoc.xsd"/>
	<xs:include schemaLocation="TaxMLPublic.xsd"/>
	<xs:complexType name="HXZGFP05188Request">
		<xs:annotation>
			<xs:documentation>根据发票种类、发票代码及发票号码查询纳税人发票领用信息</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="taxDoc">
				<xs:sequence>
					<xs:element name="fpzlDm" type="fpzlDm">
						<xs:annotation>
							<xs:documentation>发票种类代码</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="fpDm" type="fpDm">
						<xs:annotation>
							<xs:documentation>发票代码</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="fphm" type="fphm">
						<xs:annotation>
							<xs:documentation>发票号码</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
