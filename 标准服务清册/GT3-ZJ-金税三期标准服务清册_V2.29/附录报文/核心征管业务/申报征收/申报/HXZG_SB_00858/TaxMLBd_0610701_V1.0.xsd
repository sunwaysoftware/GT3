	<xs:complexType name="ggfhywxcfkndnstzmxbVO">
		<xs:annotation>
			<xs:documentation>《广告费和业务宣传费跨年度纳税调整明细表》</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="bnggfhywxcfzc" type="je">
