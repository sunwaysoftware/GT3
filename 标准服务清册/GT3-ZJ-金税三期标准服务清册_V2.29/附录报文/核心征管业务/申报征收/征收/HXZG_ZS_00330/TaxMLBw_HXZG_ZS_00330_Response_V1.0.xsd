					<xs:documentation>税款缴纳方式代码</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="yhzh" type="yhzh">
				<xs:annotation>
					<xs:documentation>银行账号</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bz" type="bz">
				<xs:annotation>
					<xs:documentation>备注信息</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
